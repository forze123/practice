@model IEnumerable<kyrsiv.Models.Messeng>
    <div style="padding-top:100px;">
        <h3 style="color:white;">@ViewBag.User</h3>
        <table class="table table-hover table-dark">
            <thead class="thead-light">
                <tr>
                    <td scope="col">#</td>
                    <td scope="col">Имя</td>
                    <td scope="col">Фамилия</td>
                    <td scope="col">Email</td>
                    <td scope="col">Заметка</td>
                    <td scope="col">Статус заявки</td>
                    <td scope="col"></td>
                    <td scope="col"></td>
                </tr>
            </thead>
            <tbody class="border-primary">
                @{
                    int i = 1;
                }
                @foreach (var messeng in Model)
                {
                    <tr>
                        <td scope="row">@i</td>
                        <td>@messeng.Name</td>
                        <td>@messeng.SyrName</td>
                        <td>@messeng.Email</td>
                        <td>@messeng.Note</td>
                        <td>@messeng.State</td>
                        <td><a href="/Account/Editing/@messeng.Id" class="text-light btn-outline-" role="button">Редактировать</a></td>
                        <td><a href="/Account/Delete/@messeng.Id" class="text-light btn-outline-" role="button">Удалить</a></td>
                    </tr>
                    {
                        i = i + 1;
                    }
                }
            </tbody>
        </table>
    </div>